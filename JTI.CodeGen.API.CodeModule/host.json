{
  "version": "2.0",
  "functionTimeout": "00:10:00",
  "extensions": {
    "durableTask": {
      "hubName": "MyTaskHub"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Error",
      "Function": "Information",
      "Host.Results": "Error",
      "Host.Aggregator": "Error",
      "Host.Triggers.DurableTask": "Error"
    }
  }
}